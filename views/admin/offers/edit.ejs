                      </div>
                      
                      <input type="text" class="form-control  input-sm" name="inserted_date"  value="<%=values.inserted_date;%>"  placeholder="inserted_date"id="inserted_date" data-inputmask="'alias': 'yyyy-mm-dd'" data-mask />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>comments_number</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="integer" class="form-control  input-sm" name="comments_number"  value="<%=values.comments_number;%>"  placeholder="comments_number"id="comments_number"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><label>extra_info</label>
                 <div class="form-group">


                      <textarea type="textarea" class="form-control  input-sm" name="extra_info"  placeholder="extra_info"id="extra_info"  style="height:150px;" ><%=values.extra_info;%> </textarea>

                  </div><!-- /.form group -->    <div class="box-footer"></div>
    <input type="submit" value="create">
</form>

<script>$("[data-mask]").inputmask();$("[data-mask]").inputmask();$("#inserted_date").inputmask("yyyy-mm-dd", {"placeholder": "yyyy-mm-dd"});</script>
