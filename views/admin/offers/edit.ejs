<% include ../include/header %>


<section id="center_page_section">
    <div class="centerPageHeader" id="center_body_header" >
        أدخل معلومات الدفعة

    </div><!-- #center_body_header -->

    <div class="block_body"  id="center_body_body">





    <form method="post" >
    <div class="form-group">
                    <label>goods_id</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="integer" class="form-control  input-sm" name="goods_id"  value="<%=values.goods_id;%>"  placeholder="goods_id"id="goods_id"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>goods_users_id</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="integer" class="form-control  input-sm" name="goods_users_id"  value="<%=values.goods_users_id;%>"  placeholder="goods_users_id"id="goods_users_id"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>users_id</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="integer" class="form-control  input-sm" name="users_id"  value="<%=values.users_id;%>"  placeholder="users_id"id="users_id"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                      <label>category</label>
                      <select type="select" class="form-control  input-sm" name="category"  id="category" ><% var offers_category=arrays("offers","category");%>
    <% for(var key in offers_category){%>
        <option value="<%=key;%>"><%=offers_category[key];%></option>
                          <% } %> </select>
                    </div><div class="form-group">
                    <label>user_name</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-pencil"></i>
                      </div>
                      
                      <input type="text" class="form-control  input-sm" name="user_name"  value="<%=values.user_name;%>"  placeholder="user_name"id="user_name"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>title</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-pencil"></i>
                      </div>
                      
                      <input type="text" class="form-control  input-sm" name="title"  value="<%=values.title;%>"  placeholder="title"id="title"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>public_price</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="number" class="form-control  input-sm" name="public_price"  value="<%=values.public_price;%>"  placeholder="public_price"id="public_price"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>last_raise</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="number" class="form-control  input-sm" name="last_raise"  value="<%=values.last_raise;%>"  placeholder="last_raise"id="last_raise"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                    <label>installment_gain</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="number" class="form-control  input-sm" name="installment_gain"  value="<%=values.installment_gain;%>"  placeholder="installment_gain"id="installment_gain"  />
                    </div><!-- /.input group -->
                  </div><!-- /.form group --><div class="form-group">
                      <label>installment_unit</label>
                      <select type="select" class="form-control  input-sm" name="installment_unit"  id="installment_unit" ><% var offers_installment_unit=arrays("offers","installment_unit");%>
