        <option value="<%=key;%>"><%=offers_installment_unit[key];%></option>
                          <% } %> </select>
                    </div><div class="form-group">
                    <label>installment_max_repeat</label>
                    <div class="input-group">
                                
                      <div class="input-group-addon">
                        <i class="fa fa-fw fa-list-ol"></i>
                      </div>
                      
                      <input type="integer" class="form-control  input-sm" name="installment_max_repeat"  value="<%=values.installment_max_repeat;%>"  placeholder="installment_max_repeat"id="installment_max_repeat"  />
