    <div class="block_body"  id="center_body_body">



        <section id="searchForm" class="<%=(search.searchButton != undefined)? '':'mini';%>">
            <form action="" method="get">
                <button type="submit" value="search"  class="searchButton" name="searchButton">search</button>







                
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="id" value="<%=search.id;%>" placeholder="id" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="users_id" value="<%=search.users_id;%>" placeholder="users_id" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="in_amount" value="<%=search.in_amount;%>" placeholder="in_amount" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="out_amount" value="<%=search.out_amount;%>" placeholder="out_amount" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="current_balance" value="<%=search.current_balance;%>" placeholder="current_balance" >
