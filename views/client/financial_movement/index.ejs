                            </ul>
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="operation_id" value="<%=search.operation_id;%>" placeholder="operation_id" >
                        </div>


                                    













            </form>
            <i class=" searchIcon fa fa-search"></i>
        </section>






