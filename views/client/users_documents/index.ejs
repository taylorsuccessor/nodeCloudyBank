<div class="table">


   <div class="thead">
      <div class="tr">

         
         <div class="th"><%-orderByLink('id',search);%></div>
         
         <div class="th"><%-orderByLink('users_id',search);%></div>
         
         <div class="th"><%-orderByLink('name',search);%></div>
         
         <div class="th"><%-orderByLink('file_type',search);%></div>
         
