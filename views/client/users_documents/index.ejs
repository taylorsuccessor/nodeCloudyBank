                            <label for="user_name"></label>
                            <input name="id" value="<%=search.id;%>" placeholder="id" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="users_id" value="<%=search.users_id;%>" placeholder="users_id" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="name" value="<%=search.name;%>" placeholder="name" >
                        </div>


                                    
                    

                        <div class="searchInputContainer">
                            <label for="user_name"></label>
                            <input name="file_type" value="<%=search.file_type;%>" placeholder="file_type" >
